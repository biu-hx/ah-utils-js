{
  "name": "ah-utils-js",
  "version": "1.0.3",
  "description": "前端工具库，支持高精度数学计算",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "pnpm run type-check && pnpm run lint:check && pnpm run format:check && pnpm run test && pnpm run build",
    "version:patch": "npm version patch -m \"chore: release v%s\"",
    "version:minor": "npm version minor -m \"chore: release v%s\"",
    "version:major": "npm version major -m \"chore: release v%s\"",
    "release:patch": "pnpm run version:patch && pnpm publish --access public",
    "release:minor": "pnpm run version:minor && pnpm publish --access public",
    "release:major": "pnpm run version:major && pnpm publish --access public",
    "release": "pnpm publish --access public"
  },
  "keywords": [
    "utils",
    "utilities",
    "helpers",
    "typescript",
    "decimal",
    "math",
    "precision",
    "array",
    "object",
    "string",
    "dom",
    "browser"
  ],
  "author": "qyss",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahe-qyss/ah-utils-js"
  },
  "bugs": {
    "url": "https://github.com/ahe-qyss/ah-utils-js/issues"
  },
  "homepage": "https://github.com/ahe-qyss/ah-utils-js#readme",
  "dependencies": {
    "decimal.js": "^10.6.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=9.0.0"
  },
  "packageManager": "pnpm@9.14.4"
}
